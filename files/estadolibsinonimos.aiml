<aiml version="2.0">
     	<!-- Sinonimos de ver estado de libranza -->   
     	
     	
  <category>
     <pattern>NO HAN CARGADO  * *</pattern>
     <template>
         <think><set name="libranza"><star index= "2"/></set></think>
         <condition name="libranza" >
             <li value="libranza">
                 <srai>
                     Consulcre
                 </srai>
             </li>
             
            <li value="libransa">
                 <srai>
