<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
    	<!-- Sinonimos de pin -->
    	
    	
  <category>
     <pattern>NO ME HA LLEGADO * *</pattern>
     <template>
         <think><set name="pin"><star index= "2"/></set></think>
         <condition name="pin" >
             <li value="pink">
                 <srai>
                     _ PIN
                 </srai>
             </li>
             
            <li value="pun">
                 <srai>
                     _ PIN
                 </srai>
             </li>
             
            <li value="codigo">
                 <srai>
                     _ PIN
                 </srai>
             </li>

         </condition>
     </template>
 </category>
    	

