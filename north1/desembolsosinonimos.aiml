<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
	<!-- Sinonimos desembolso  -->
	
  <category>
     <pattern>NECESITO CONSULTAR * *</pattern>
     <template>
         <think><set name="clave"><star index= "2"/></set></think>
         <condition name="clave" >
             <li value="desprendible">
                 <srai>
                     desprend
                 </srai>
             </li>
             <li value="certificado">
                <button>
                    <text>Tipos de certificado</text>
                    <postback>Tipcert</postback>
                </button>
             </li>
             <li value="comprobante">
                  <srai>
                     desprend
                 </srai>
             </li>
         </condition>
     </template>
 </category>
 
