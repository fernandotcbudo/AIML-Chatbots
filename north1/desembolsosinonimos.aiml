 	<!-- Postbacks certificado  -->
 	
 	<category>
 	    <pattern>Tipcert</pattern>
 	    <template>
 	        <button>
 	            <text>Certificado de nomina</text>
 	            <postback>Certnomin</postback>
             </button>
             
             <button>
 	            <text>Certificado de asignación de retiro</text>
 	            <postback>Certasig</postback>
             </button>
             
            <button>
 	            <text>Certificado de retención en la fuente</text>
 	            <postback>Certrete</postback>
             </button>
             
 	    </template>
     </category>
     
     <category>
         <pattern>Certnomin</pattern>
         <template>Su certificado de nomina puede ser consultado en CREMIL</template>
     </category>
     
    <category>
         <pattern>Certasig</pattern>
         <template>Para este certificado por favor comuniquese con CREMIL al telefono 3537300</template>
     </category>
     
     <category>
         <pattern>Certrete</pattern>
         <template>Para este certificado por favor comuniquese con CREMIL al telefono 3537300</template>
     </category>
     
    
 
</aiml>